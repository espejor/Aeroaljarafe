<ul id="listado" class="list-group">
    <li class="list-group-item list-group-item-info">
        <h3 class="list-group-item-header">Aviones</h3>
    </li>
        {{# each planes}}
            <li class="list-group-item">
                {{_id}}
                {{model.brand.brand}}
                {{model.model}}
                {{plate}}
                {{hours}}
                {{status}}
            </li>
        {{/each}}
</ul>